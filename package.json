    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^4.1.2",
    "sass": "^1.58.3",
    "turbo": "latest",
    "typescript": "~4.7.4",
    "unocss": "^0.50.1",
    "unplugin-auto-import": "^0.15.0",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  },
  "packageManager": "pnpm@7.27.0"
